    <javascript id="8cea0364-e917-40a1-800c-faac63094e0a"><![CDATA[function (e) {

    const currentDate = new Date();

    const year = currentDate.getFullYear();
    const month = (currentDate.getMonth() + 1).toString().padStart(2, '0');
    const day = currentDate.getDate().toString().padStart(2, '0');

    const formattedDate = `${year}${month}${day}`;

    if (self.TXT_KOR_NM3.text() == "") {
        self.TXT_KOR_NM3.text(self.user.username);
        self.TXT_ISSUE_DT3.text(formattedDate);
        self.TXT_DEPT_NM3.text(self.user.deptName);
        self.TXT_PSTN_CD3.text(self.user.dutyName);

        self.bindingForm4.bindTo(self.mstGrid);

        self.mstGrid.setCellValue(self.mstGrid.select(), 'ISSUE_EMP_NO3', self.user.empCode);
    } else {
        self.bindingForm4.bindTo(self.mstGrid);
    }
}]]></javascript>
