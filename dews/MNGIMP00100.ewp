    <javascript id="6bf1f6d6-3ecc-44b7-9427-dadc78ef5864"><![CDATA[function formatDate(inputDate) {
    const year = inputDate.getFullYear();
    const month = String(inputDate.getMonth() + 1).padStart(2, '0');
    const day = String(inputDate.getDate()).padStart(2, '0');

    const formattedDate = `${year}-${month}-${day}`;
    return formattedDate;
}

let componentBlock = function (isEnable) {
    self.multifilecomponent2.uploadEnable(isEnable);
    self.filecomponent2.enable(isEnable);
}

]]></javascript>
    <javascript id="6fe5086e-0b81-4457-a337-d798abb45946"><![CDATA[function (e) {
