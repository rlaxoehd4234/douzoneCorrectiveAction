    <javascript id="6396ed89-feda-4c58-8560-cb0f0aa7c49a"><![CDATA[function (e) {
    try {
        if (e.mode == 'complete') {
            isUploading = false;

            // 모든 파일 업로드 성공 시
            if (e.total == e.success) {
                var items = [];
                var item;

                for (var i = 0; i < e.files.length; i++) {
                    item = {
                        identifier: self.mstGrid.getCellValue(self.mstGrid.select(), 'ATCHFILE_ID')
                        , originalExtension: e.files[i].extension
                        , fileSize: e.files[i].size
                        , originalFilename: e.files[i].filename
                        , newFilename: e.files[i].fileKey
                        , FILE_ATCH_TXT: '/EH/UploadFile'
                        , COMPANY_CD: self.mstGrid.getCellValue(self.mstGrid.select(), 'COMPANY_CD')
                        , SQ_NO: self.mstGrid.getCellValue(self.mstGrid.select(), 'SQ_NO')
                        , CRCT_TRMT_NO: self.mstGrid.getCellValue(self.mstGrid.select(), 'CRCT_TRMT_NO')
                    }
                    items[i] = item;
                }
                console.log(items);

                dews.api.post(dews.url.getApiUrl('EH', 'AbcCommonService', 'abcMultiFile_save'), {
                    async: false,
                    data: {
                        abcMultiFile_ds: JSON.stringify(items)
                        , moduleCode: '100'
                    }
                }).done(function (data) {
                    dews.ui.snackbar.ok('자료가 정상적으로 저장되었습니다.');

                    console.log(data);
                    let atchId = data.MSG;
                    self.mstGrid.setCellValue(self.mstGrid.select(), 'ATCHFILE_ID', atchId);

                    // self.abc00100MultiFile_ds.read();
                }).fail(function (xhr, status, error) {
                    dews.error('저장이 실패하였습니다.', 'ico1');
                    dews.ui.snackbar.error(error);
                });
                dews.ui.snackbar.ok('모든 파일 업로드');
            }
            else {
                dews.ui.snackbar.warning('업로드 실패');
            }
        }

    } catch (error) {
        console.log(error);
        dews.error(error);
    }

}]]></javascript>
